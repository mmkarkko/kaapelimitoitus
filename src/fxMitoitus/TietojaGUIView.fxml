<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="fxMitoitus.TietojaGUIController">
   <center>
      <VBox alignment="CENTER" prefHeight="250.0" prefWidth="350.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="labelNimi" alignment="CENTER" text="Kaapelimitoitus">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="30.0" />
               </VBox.margin>
            </Label>
            <Label alignment="CENTER" text="Versio 1.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label alignment="CENTER" text="Miia Arkko">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Label>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleSulje" text="OK" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="30.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="7.0" left="15.0" right="15.0" top="7.0" />
               </padding>
            </Button>
         </children>
      </VBox>
   </center>
</BorderPane>
